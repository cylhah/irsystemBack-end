<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cylwyc.demo.dao.UserAndCollectionDao">
    <insert id="insertUserAndCollection">
        INSERT INTO
        userAndCollection(userId,articleId,collectTime)
        VALUES
        (#{userId},#{articleId},#{collectTime})
    </insert>
    <delete id="deleteArea">
        DELETE FROM
        userAndCollection
        WHERE
        articleId=#{articleId}
        AND userId=#{userId}
    </delete>
</mapper>